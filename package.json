{
  "name": "prideflagsdotinfo",
  "description": "A website for finding different LGBTQ+ prideflags and explanations about queer identities.",
  "version": "0.6.0",
  "private": true,
  "engines": {
    "node": ">=12.0.0"
  },
  "scripts": {
    "dev": "sapper dev",
    "build": "sapper build",
    "build:legacy": "sapper build --legacy",
    "export": "sapper export",
    "export:legacy": "sapper export --legacy",
    "start": "node __sapper__/build",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "cy": "run-p --race dev cy:open",
    "test": "run-p --race dev cy:run"
  },
  "dependencies": {
    "compression": "^1.7.1",
    "lodash": "^4.17.15",
    "polka": "next",
    "sirv": "^0.4.0",
    "svelte-media-query": "^0.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "cypress": "^4.12.1",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.26",
    "postcss-custom-media": "^7.0.8",
    "postcss-load-config": "^2.1.0",
    "postcss-media-minmax": "^4.0.0",
    "rollup": "^1.12.0",
    "rollup-plugin-babel": "^4.0.2",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-svelte": "^5.0.1",
    "rollup-plugin-terser": "^4.0.4",
    "sapper": "^0.27.0",
    "svelte": "^3.0.0",
    "svelte-preprocess": "^3.4.0"
  }
